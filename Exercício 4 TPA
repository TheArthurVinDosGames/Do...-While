public class AnosBissextos {

    public static void main(String[] args) {
        System.out.println("Anos bissextos do ano 1 ao 2023:");

        for (int ano = 1; ano <= 2023; ano++) {
            if ((ano % 4 == 0) && ((ano % 100 != 0) || (ano % 400 == 0))) {
                System.out.println(ano);
            }
        }
    }
}
