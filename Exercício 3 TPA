public class MultiplosDeDez {

    public static void main(String[] args) {
        for (int c = 1; c <= 100; c++) {
            if (contador % 10 == 0) {
                System.out.println(c + " é múltiplo de 10");
            } else {
                System.out.println(c);
            }
        }
    }
}
